{
  "name": "export-figma-svg",
  "version": "1.0.0",
  "description": "Export SVGs from Figma and build platforms",
  "main": "index.ts",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "pnpm run build:react && pnpm run build:android",
    "build:svgs": "ts-node src/index.ts",
    "build:react": "svgr --out-dir dist/react svgs --config-file svgr.config.json",
    "build:android": "s2v -f svgs -o dist/android",
    "test": "jest"
  },
  "author": "Tom Genoni <tom@genoni.studio>",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.4.5",
    "fs": "0.0.1-security",
    "pg": "^8.13.0",
    "svg2vectordrawable": "^2.9.1",
    "ts-node": "^10.9.2",
    "tsc": "^2.0.4",
    "typescript": "^5.6.2"
  },
  "devDependencies": {
    "@svgr/cli": "^8.1.0",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5"
  },
  "packageManager": "pnpm@9.1.1+sha512.14e915759c11f77eac07faba4d019c193ec8637229e62ec99eefb7cf3c3b75c64447882b7c485142451ee3a6b408059cdfb7b7fa0341b975f12d0f7629c71195"
}
